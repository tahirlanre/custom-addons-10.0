<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
	    <record id="sequence_delivery_batch" model="ir.sequence">
	        <field name="name">Delivery sequence</field>
	        <field name="code">delivery.sequence</field>
	        <field name="prefix">DEL</field>
	        <field eval="1" name="number_next"/>
	        <field eval="1" name="number_increment"/>
	        <field eval="True" name="use_date_range"/>
	        <field eval="False" name="company_id"/>
	        <field name="padding">4</field>
	    </record>
	</data>
    
    <data noupdate="1">
        <record id="delivery_product" model="product.product">
            <field name="name">Delivery</field>
            <field name="standard_price">0.0</field>
            <field name="list_price">0.0</field>
            <field name="type">service</field>
            <field name="default_code">DL00</field>
            <field name="sale_line_warn">no-message</field>
        </record>
    </data>
</odoo>
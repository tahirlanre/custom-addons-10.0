<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
	    <record id="sequence_journal_batch" model="ir.sequence">
	        <field name="name">Journal batch sequence</field>
	        <field name="code">account.journal.batch</field>
	        <field name="prefix">JB</field>
	        <field eval="1" name="number_next"/>
	        <field eval="1" name="number_increment"/>
	        <field eval="False" name="company_id"/>
	        <field name="padding">3</field>
	    </record>
		
	    <record id="sequence_journal_batch_line" model="ir.sequence">
	        <field name="name">Journal batch line sequence</field>
	        <field name="code">account.journal.batch.line</field>
	        <field name="prefix">JBR</field>
	        <field eval="1" name="number_next"/>
	        <field eval="1" name="number_increment"/>
	        <field eval="False" name="company_id"/>
	        <field name="padding">3</field>
	    </record>
	</data>
	
</odoo>
